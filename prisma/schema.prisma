// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model books {
  id              String @id @default(auto()) @map("_id") @db.ObjectId
  title           String
  author          String
  publicationYear Int
  isbn            String
}
